{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "license": "<%= license %>",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
<%_ if (federationPluginImported && federationType == 'Remote') { _%>
    "serve": "vite preview --port 3072"
<%_ } else { _%>
    "serve": "vite preview",
<%_ } _%>
    "test": "jest"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-i18n": "^9.2.0-beta.15",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    <%_ plugins.forEach(function(plugin) { _%>
    "<%= plugin.package %>":"latest",
    <%_ }) _%>
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@types/jest": "^26.0.24",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.16",
    "babel-jest": "^26.5.6",
    "jest": "^26.5.6",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.2.2"
  }
}
