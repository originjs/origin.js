{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "license": "<%= license %>",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
<%_ if (federationPluginImported && federationType == 'Remote') { _%>
    "serve": "vite preview --port 3072"
<%_ } else { _%>
    "serve": "vite preview"
<%_ } _%>
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-i18n": "^9.2.0-beta.15",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    <%_ plugins.forEach(function(plugin) { _%>
    "<%= plugin.package %>":"latest",
    <%_ }) _%>
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.6.13",
    "vue-tsc": "^0.2.2"
  }
}
